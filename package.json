{
  "name": "trello-trieuninhhan-api",
  "version": "1.0.0",
  "description": "APIs for trello clone app",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "remove-build": "rmdir /s /q build",
    "create-build": "mkdir build",
    "clean": "npm run remove-build && npm run create-build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/server.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/server.js",
    "start": "node ./build/src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TranNinhHieu/trello-trieuninhhan-api.git"
  },
  "keywords": [],
  "author": "trieuninhhan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TranNinhHieu/trello-trieuninhhan-api/issues"
  },
  "homepage": "https://github.com/TranNinhHieu/trello-trieuninhhan-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "nodemon": "^2.0.15"
  }
}
